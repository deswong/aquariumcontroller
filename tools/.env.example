# Aquarium Water Change ML Service Configuration
# Copy this to .env and customize

# MQTT Broker Configuration
MQTT_BROKER=localhost
MQTT_PORT=1883
MQTT_USER=
MQTT_PASSWORD=

# MQTT Topic Prefix (must match ESP32 configuration)
MQTT_TOPIC_PREFIX=aquarium

# MariaDB Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_NAME=aquarium
DB_USER=aquarium
DB_PASSWORD=

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
